    "# calling function with names and grades\n",
    "\n",
    "def avg_grades(stud_grade):\n",
    "    total = 0\n",
    "    for nam , grad in stud_grade.items():\n",
    "        print(f\"{nam}, {grad}\" )\n",
    "        total += grad\n",
    "    return total/len(stud_grade)\n",
    "\n",
